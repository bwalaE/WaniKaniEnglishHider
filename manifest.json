{
  "manifest_version": 3,
  "name": "WaniKani Example Text Hider",
  "description": "Hides the English translations of example sentences in Wanikani so you can try to guess them and then reveal",
  "version": "1.0",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [{
    "matches": ["https://www.wanikani.com/*"],
    "js": ["jquery-3.6.4.min.js", "contentScript.js"]
  }],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icons/128.png"
  }
}